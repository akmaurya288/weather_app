{
  "expo": {
    "name": "Weather",
    "slug": "weather_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons8-star-96.png",
    "splash": {
      "image": "./assets/icons8-star-96.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "space.krida.weather",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "space.krida.weather",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}